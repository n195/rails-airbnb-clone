<%= form_for(restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.label :creator %>
  <%= f.text_field :creator %>
  <%= f.label :creation_date %>
  <%= f.number_field :creation_date %>
  <%= f.label :description %>
  <%= f.text_field :description %>
<% end %>



